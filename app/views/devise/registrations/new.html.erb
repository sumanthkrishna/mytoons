<h2>Sign up</h2>
<br/>
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>

<!--   <%= devise_error_messages! %> -->
<table class="reg_table">
	<tr>
 <td><%= f.label :firstname %></td>
  <td><%= f.text_field :first_name %></td>
</tr>
<tr><td><%= f.label :lastname %></td>
  <td><%= f.text_field :last_name %></td>
</tr>
<tr><td><%= f.label :Phone_Number %></td>
  <td><%= f.text_field :phone_number %></td>
</tr>

<tr><td><%= f.label :email %></td>
  <td><%= f.email_field :email %></td>
</tr>
<tr><td><%= f.label :password %></td>
 	<td> <%= f.password_field :password %></td>
</tr>
  <tr><td><%= f.label :password_confirmation %></td>
 <td> <%= f.password_field :password_confirmation %></td>
</tr>
 <tr><td></td><td ><%= f.submit "Sign up" ,:class=>"btn btn-primary", :style=>"margin-left: 157px;"%></td></tr></table>
<% end %>
<%#= render "devise/shared/links" %>
